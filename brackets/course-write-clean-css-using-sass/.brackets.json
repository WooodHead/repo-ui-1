{
    "scsslint.gemDir": "/usr/local/bin/",
    "sass.enabled": false,
    "path": {
        "**/scss/*.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css/",
                "includePaths": [],
                "sourceComments": true,
                "outputStyle": "expanded"
            },
            "linting.collapsed": false,
            "mikaeljorhult.bracketsPHPLintTools.php-available": true
        }
    }
}
